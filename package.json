{
  "version": "0.1.1",
  "name": "abr-2015",
  "title": "The Afro Banana Republic",
  "description": "The Afro Banana Republic (ABR)",
  "keywords": "alternative music festival cyprus",
  "baseurl": "//jekyll-grunt-template.rudasn.org",
  "GoogleAnalytics": "",
  "build_dir": "_site",
  "serve_dir": ".serve",
  "assets_dir": "static",
  "assets": {
    "images": "/static/images/",
    "css": "/static/css/",
    "js": "/static/js/"
  },
  "permalink": "/:categories/:year/:month-:title/",
  "exclude": [
    "README.md",
    "gruntfile.js",
    "package.json",
    "node_modules",
    "Gemfile",
    "Gemfile.lock"
  ],
  "image": {
    "source": "static/images/_src",
    "output": "static/images",
    "presets": {
      "default": {},
      "lineup": {
        "width": "344",
        "height": "AUTO"
      }
    }
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-contrib-concat": "^0.3.0",
    "grunt-contrib-uglify": "^0.4.1",
    "grunt-contrib-imagemin": "^0.4.1",
    "grunt-contrib-cssmin": "^0.9.0",
    "grunt-shell": "^0.6.4",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-convert": "~0.1.10",
    "grunt-usemin": "^2.6.2",
    "grunt-contrib-connect": "^0.7.1",
    "grunt-gh-pages": "^0.9.1",
    "grunt-rev": "~0.1.0",
    "grunt-contrib-htmlmin": "^0.2.0",
    "grunt-contrib-jshint": "^0.9.2",
    "grunt-contrib-csslint": "^0.2.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-rename": "~0.1.3",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-manifest": "^0.4.0",
    "grunt-newer": "^1.1.0"
  }
}
